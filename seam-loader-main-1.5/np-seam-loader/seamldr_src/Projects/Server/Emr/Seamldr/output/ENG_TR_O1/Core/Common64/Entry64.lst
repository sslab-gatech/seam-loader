   781                              <1> ;            Implementation of if and else Macros
   782                              <1> ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
   783                              <1> 
   784                              <1> %macro IF 1
   785                              <1> 
   786                              <1>         %push IF
   787                              <1>         j%-1  %$ifnot
   788                              <1> 
   789                              <1> %endmacro
   790                              <1> 
   791                              <1> %macro ELSE 0
   792                              <1> 
   793                              <1>   %ifctx IF
   794                              <1>         %repl   else
   795                              <1>         jmp     %$ifend
   796                              <1>         %$ifnot:
   797                              <1>   %else
   798                              <1>         %error  "expected IF before ELSE"
   799                              <1>   %endif
   800                              <1> 
   801                              <1> %endmacro
   802                              <1> 
   803                              <1> %macro ENDIF 0
   804                              <1> 
   805                              <1>   %ifctx IF
   806                              <1>         %$ifnot:
   807                              <1>         %pop
   808                              <1>   %elifctx  else
   809                              <1>         %$ifend:
   810                              <1>         %pop
   811                              <1>   %else
   812                              <1>         %error  "expected IF or ELSE before ENDIF"
   813                              <1>   %endif
   814                              <1> 
   815                              <1> %endmacro

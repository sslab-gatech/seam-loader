    47                                   ;;; SeamldrThunk64 :
    48                                          ;
    49                                          ; System in compatibility mode
    50                                          ;
    51                                          mov     ecx, ACM_CODE64_SELECTOR ;ACM64_CODE
    51 00000000 B918000000               ;;;  mov ecx, 24
    52                                          push    ecx                     ; push ecx - in 32 bit mode
    52 00000005 51                       ;;;  push ecx
    53                                  
    54                                          mov     ecx, LongMode
    54 00000006 B9[0D000000]             ;;;  mov ecx, LongMode
    55                                          push    ecx                     ; push ecx - in 32 bit mode
    55 0000000B 51                       ;;;  push ecx
    56                                          retf                            ; will jump to LongMode label below
    56 0000000C CB                       ;;;  retf
    57                                          ;
    58                                          ; Long mode.
    59                                          ;
    60                                  LongMode:                     
    60                                   ;;; LongMode:
    61                                          db      48h, 0B8h
    61 0000000D 48B8                     ;;;  db 48h, 0B8h
    62                                          dq      0FFFFFFFFh              ; mov   rax, 00000000FFFFFFFFh
    62 0000000F FFFFFFFF00000000         ;;;  dq 0FFFFFFFFh
    63                                          db      48h, 21h, 0C4h          ; and   rsp, rax
    63 00000017 4821C4                   ;;;  db 48h, 21h, 0C4h
